ZMK_MACRO( _ ,
        bindings = <&trans>;
        )

ZMK_MACRO(ditto,
        wait-ms = <10>;
        tap-ms = <20>;
        bindings
        = <&macro_press &kp LGUI &kp LALT>
        , <&macro_tap &kp GRAVE>
        , <&macro_release &kp LGUI &kp LALT>
        ;
        )

ZMK_MACRO(T_ADD,
        wait-ms = <0>;
        tap-ms = <20>;
        bindings = <&kp LG(LA(Q))>;
        )
ZMK_MACRO(T_OPEN,
        wait-ms = <0>;
